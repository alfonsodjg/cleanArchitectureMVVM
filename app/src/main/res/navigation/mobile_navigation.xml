<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ramdomuser.ui.views.UserFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_user" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.ramdomuser.ui.views.EmailFragment"
        android:label="@string/title_email"
        tools:layout="@layout/fragment_email" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.ramdomuser.ui.views.BirthdayFragment"
        android:label="@string/title_bithday"
        tools:layout="@layout/fragment_birthday" />
</navigation>